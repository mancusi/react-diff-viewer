{
	"name": "@mancusi/react-diff-viewer",
	"version": "3.3.0",
	"private": false,
	"description": "A simple and beautiful text diff viewer component made with diff and React",
	"keywords": [
		"review",
		"code-review",
		"diff",
		"diff-viewer",
		"github",
		"react",
		"react-component",
		"ui"
	],
	"repository": "git@github.com:praneshr/react-diff-viewer.git",
	"license": "MIT",
	"author": "Pranesh Ravi<praneshpranesh@gmail.com>",
	"main": "lib/index",
	"typings": "lib/index",
	"scripts": {
		"build": "tsc --outDir lib/",
		"build:examples": "webpack --progress --colors",
		"build:watch": "tsc --outDir lib/ -w",
		"publish:examples": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist -r $GITHUB_REPO_URL",
		"publish:examples:local": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist",
		"start:examples": "webpack-dev-server --open --hot",
		"test": "mocha --require ts-node/register --require enzyme.ts ./test/**",
		"test:watch": "mocha --require ts-node/register --require enzyme.ts --watch-extensions ts,tsx --watch ./test/**"
	},
	"dependencies": {
		"@emotion/css": "^11.7.1",
		"@emotion/serialize": "^1.0.2",
		"classnames": "^2.2.6",
		"diff": "^4.0.1",
		"memoize-one": "^5.0.4",
		"prop-types": "^15.6.2"
	},
	"devDependencies": {
		"@types/classnames": "^2.2.6",
		"@types/diff": "^4.0.2",
		"@types/enzyme": "^3.1.14",
		"@types/enzyme-adapter-react-16": "^1.0.3",
		"@types/expect": "^1.20.3",
		"@types/memoize-one": "^4.1.1",
		"@types/mocha": "^5.2.5",
		"@types/node": "^12.0.12",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^1.11.0",
		"@typescript-eslint/parser": "^1.11.0",
		"css-loader": "^6.7.1",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.6.0",
		"eslint": "6.0.1",
		"eslint-config-airbnb": "17.1.1",
		"eslint-plugin-import": "^2.18.0",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.14.2",
		"expect": "^24.8.0",
		"favicons-webpack-plugin": "^5.0.2",
		"file-loader": "^4.0.0",
		"gh-pages": "^2.0.1",
		"html-webpack-plugin": "^5.5.0",
		"mini-css-extract-plugin": "^2.6.1",
		"mocha": "^6.1.4",
		"node-sass": "^7.0.1",
		"raw-loader": "^4.0.2",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"sass-loader": "^13.0.2",
		"spy": "^1.0.0",
		"ts-loader": "^9.3.1",
		"ts-node": "^8.3.0",
		"typescript": "^3.5.2",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.3"
	},
	"peerDependencies": {
		"react": "^15.3.0 || ^16.0.0",
		"react-dom": "^15.3.0 || ^16.0.0"
	},
	"engines": {
		"node": ">= 8"
	}
}
